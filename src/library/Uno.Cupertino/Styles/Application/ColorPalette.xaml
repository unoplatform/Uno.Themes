<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<!--
		This palette contains the default colors and opacity levels for the different state overlays
		All of them can be overriden by the application consuming this library

		All variations of this palette are offered as brushes in Colors.xaml.
	-->

	<ResourceDictionary.ThemeDictionaries>

		<!-- Light Theme -->
		<ResourceDictionary x:Key="Light">
			<!-- Basic colors -->
			<Color x:Key="CupertinoBlueColor">#007BFF</Color>
			<Color x:Key="CupertinoGreenColor">#34C759</Color>
			<Color x:Key="CupertinoIndigoColor">#5856D6</Color>
			<Color x:Key="CupertinoOrangeColor">#FF9500</Color>
			<Color x:Key="CupertinoPinkColor">#FF2D54</Color>
			<Color x:Key="CupertinoPurpleColor">#AF52DE</Color>
			<Color x:Key="CupertinoRedColor">#FF3B30</Color>
			<Color x:Key="CupertinoTealColor">#5AC7FA</Color>
			<Color x:Key="CupertinoYellowColor">#FFCC00</Color>
			<Color x:Key="CupertinoWhiteColor">#FFFFFF</Color>
			<Color x:Key="CupertinoBlackColor">#000000</Color>

			<!-- Types of gray colors -->
			<Color x:Key="CupertinoPrimaryGrayColor">#8E8E93</Color>
			<Color x:Key="CupertinoSecondaryGrayColor">#AEAEB2</Color>
			<Color x:Key="CupertinoTertiaryGrayColor">#C7C7CC</Color>
			<Color x:Key="CupertinoQuaternaryGrayColor">#D1D1D6</Color>
			<Color x:Key="CupertinoQuinaryGrayColor">#E5E5EA</Color>
			<Color x:Key="CupertinoSenaryGrayColor">#F2F2F7</Color>

			<!-- Controls colors -->
			<Color x:Key="LabelColor">#000000</Color>
			<Color x:Key="SecondaryLabelColor">#993c3c43</Color>
			<Color x:Key="TertiaryLabelColor">#4c3c3c43</Color>
			<Color x:Key="QuaternaryLabelColor">#2d3c3c43</Color>
			<Color x:Key="SystemFillColor">#33787880</Color>
			<Color x:Key="SecondarySystemFillColor">#28787880</Color>
			<Color x:Key="TertiarySystemFillColor">#1e767680</Color>
			<Color x:Key="QuaternarySystemFillColor">#14747480</Color>
			<Color x:Key="PlaceholderTextColor">#4c3c3c43</Color>
			<Color x:Key="SystemBackgroundColor">#FFFFFF</Color>
			<Color x:Key="SecondarySystemBackgroundColor">#f2f2f7</Color>
			<Color x:Key="TertiarySystemBackgroundColor">#FFFFFF</Color>
			<Color x:Key="SystemGroupedBackgroundColor">#f2f2f7</Color>
			<Color x:Key="SecondarySystemGroupedBackgroundColor">#FFFFFF</Color>
			<Color x:Key="TertiarySystemGroupedBackgroundColor">#f2f2f7</Color>
			<Color x:Key="SeparatorColor">#493c3c43</Color>
			<Color x:Key="OpaqueSeparatorColor">#c6c6c8</Color>
			<Color x:Key="LinkColor">#007aff</Color>
			<Color x:Key="RadioButtonBackgroundColor">#FFFFFF</Color>
		</ResourceDictionary>

		<!-- Dark Theme -->
		<ResourceDictionary x:Key="Dark">
			<!-- Basic colors -->
			<Color x:Key="CupertinoBlueColor">#0A84FF</Color>
			<Color x:Key="CupertinoGreenColor">#30D158</Color>
			<Color x:Key="CupertinoIndigoColor">#5E5CE6</Color>
			<Color x:Key="CupertinoOrangeColor">#FF9F0A</Color>
			<Color x:Key="CupertinoPinkColor">#FF375F</Color>
			<Color x:Key="CupertinoPurpleColor">#BF5AF2</Color>
			<Color x:Key="CupertinoRedColor">#FF443A</Color>
			<Color x:Key="CupertinoTealColor">#64D3FF</Color>
			<Color x:Key="CupertinoYellowColor">#FFD60A</Color>
			<Color x:Key="CupertinoWhiteColor">#FFFFFF</Color>
			<Color x:Key="CupertinoBlackColor">#000000</Color>

			<!-- Types of gray colors -->
			<Color x:Key="CupertinoPrimaryGrayColor">#8E8E93</Color>
			<Color x:Key="CupertinoSecondaryGrayColor">#636366</Color>
			<Color x:Key="CupertinoTertiaryGrayColor">#48484A</Color>
			<Color x:Key="CupertinoQuaternaryGrayColor">#3A3A3C</Color>
			<Color x:Key="CupertinoQuinaryGrayColor">#2C2C2E</Color>
			<Color x:Key="CupertinoSenaryGrayColor">#1C1C1E</Color>

			<!-- Controls colors -->
			<Color x:Key="LabelColor">#FFFFFF</Color>
			<Color x:Key="SecondaryLabelColor">#99ebebf5</Color>
			<Color x:Key="TertiaryLabelColor">#4cebebf5</Color>
			<Color x:Key="QuaternaryLabelColor">#2debebf5</Color>
			<Color x:Key="SystemFillColor">#5b787880</Color>
			<Color x:Key="SecondarySystemFillColor">#51787880</Color>
			<Color x:Key="TertiarySystemFillColor">#3d767680</Color>
			<Color x:Key="QuaternarySystemFillColor">#2d767680</Color>
			<Color x:Key="PlaceholderTextColor">#4cebebf5</Color>
			<Color x:Key="SystemBackgroundColor">#000000</Color>
			<Color x:Key="SecondarySystemBackgroundColor">#1c1c1e</Color>
			<Color x:Key="TertiarySystemBackgroundColor">#2c2c2e</Color>
			<Color x:Key="SystemGroupedBackgroundColor">#000000</Color>
			<Color x:Key="SecondarySystemGroupedBackgroundColor">#1c1c1e</Color>
			<Color x:Key="TertiarySystemGroupedBackgroundColor">#2c2c2e</Color>
			<Color x:Key="SeparatorColor">#99545458</Color>
			<Color x:Key="OpaqueSeparatorColor">#38383a</Color>
			<Color x:Key="LinkColor">#0984ff</Color>
			<Color x:Key="RadioButtonBackgroundColor">#8E8E93</Color>
		</ResourceDictionary>
	</ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
