<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<ResourceDictionary.ThemeDictionaries>
		<ResourceDictionary x:Key="Default">

			<!-- DISPLAY -->

			<!-- Display Large (Hero) -->
			<StaticResource x:Key="DisplayLargeFontFamily" ResourceKey="MaterialRegularFontFamily" />
			<x:String x:Key="DisplayLargeFontWeight">Bold</x:String>
			<x:Double x:Key="DisplayLargeFontSize">72</x:Double>
			<x:Int32 x:Key="DisplayLargeCharacterSpacing">0</x:Int32>

			<!-- Display Medium (Page Title) -->
			<StaticResource x:Key="DisplayMediumFontFamily" ResourceKey="MaterialRegularFontFamily" />
			<x:String x:Key="DisplayMediumFontWeight">Bold</x:String>
			<x:Double x:Key="DisplayMediumFontSize">48</x:Double>
			<x:Int32 x:Key="DisplayMediumCharacterSpacing">0</x:Int32>

			<!-- Display Small (Subtitle) -->
			<StaticResource x:Key="DisplaySmallFontFamily" ResourceKey="MaterialRegularFontFamily" />
			<x:String x:Key="DisplaySmallFontWeight">Normal</x:String>
			<x:Double x:Key="DisplaySmallFontSize">32</x:Double>
			<x:Int32 x:Key="DisplaySmallCharacterSpacing">0</x:Int32>

			<!-- HEADLINE -->

			<!-- Headline Large (Heading) -->
			<StaticResource x:Key="HeadlineLargeFontFamily" ResourceKey="MaterialRegularFontFamily" />
			<x:String x:Key="HeadlineLargeFontWeight">SemiBold</x:String>
			<x:Double x:Key="HeadlineLargeFontSize">24</x:Double>
			<x:Int32 x:Key="HeadlineLargeCharacterSpacing">0</x:Int32>

			<!-- Headline Medium (Subheading) -->
			<StaticResource x:Key="HeadlineMediumFontFamily" ResourceKey="MaterialRegularFontFamily" />
			<x:String x:Key="HeadlineMediumFontWeight">Normal</x:String>
			<x:Double x:Key="HeadlineMediumFontSize">20</x:Double>
			<x:Int32 x:Key="HeadlineMediumCharacterSpacing">0</x:Int32>

			<!-- Headline Small -->
			<StaticResource x:Key="HeadlineSmallFontFamily" ResourceKey="MaterialRegularFontFamily" />
			<x:String x:Key="HeadlineSmallFontWeight">Normal</x:String>
			<x:Double x:Key="HeadlineSmallFontSize">18</x:Double>
			<x:Int32 x:Key="HeadlineSmallCharacterSpacing">0</x:Int32>

			<!-- TITLE -->

			<!-- Title Large -->
			<StaticResource x:Key="TitleLargeFontFamily" ResourceKey="MaterialRegularFontFamily" />
			<x:String x:Key="TitleLargeFontWeight">Normal</x:String>
			<x:Double x:Key="TitleLargeFontSize">22</x:Double>
			<x:Int32 x:Key="TitleLargeCharacterSpacing">0</x:Int32>

			<!-- Title Medium -->
			<StaticResource x:Key="TitleMediumFontFamily" ResourceKey="MaterialRegularFontFamily" />
			<x:String x:Key="TitleMediumFontWeight">Medium</x:String>
			<x:Double x:Key="TitleMediumFontSize">16</x:Double>
			<x:Int32 x:Key="TitleMediumCharacterSpacing">150</x:Int32>

			<!-- Title Small -->
			<StaticResource x:Key="TitleSmallFontFamily" ResourceKey="MaterialRegularFontFamily" />
			<x:String x:Key="TitleSmallFontWeight">Medium</x:String>
			<x:Double x:Key="TitleSmallFontSize">14</x:Double>
			<x:Int32 x:Key="TitleSmallCharacterSpacing">100</x:Int32>

			<!-- LABEL -->

			<!-- Label Large -->
			<StaticResource x:Key="LabelLargeFontFamily" ResourceKey="MaterialRegularFontFamily" />
			<x:String x:Key="LabelLargeFontWeight">Medium</x:String>
			<x:Double x:Key="LabelLargeFontSize">14</x:Double>
			<x:Int32 x:Key="LabelLargeCharacterSpacing">100</x:Int32>

			<!-- Label Medium -->
			<StaticResource x:Key="LabelMediumFontFamily" ResourceKey="MaterialRegularFontFamily" />
			<x:String x:Key="LabelMediumFontWeight">Medium</x:String>
			<x:Double x:Key="LabelMediumFontSize">12</x:Double>
			<x:Int32 x:Key="LabelMediumCharacterSpacing">500</x:Int32>

			<!-- Label Small -->
			<StaticResource x:Key="LabelSmallFontFamily" ResourceKey="MaterialRegularFontFamily" />
			<x:String x:Key="LabelSmallFontWeight">Medium</x:String>
			<x:Double x:Key="LabelSmallFontSize">11</x:Double>
			<x:Int32 x:Key="LabelSmallCharacterSpacing">500</x:Int32>

			<!-- BODY -->

			<!-- Body Large (Body Base) -->
			<StaticResource x:Key="BodyLargeFontFamily" ResourceKey="MaterialRegularFontFamily" />
			<x:String x:Key="BodyLargeFontWeight">Normal</x:String>
			<x:Double x:Key="BodyLargeFontSize">16</x:Double>
			<x:Int32 x:Key="BodyLargeCharacterSpacing">500</x:Int32>

			<!-- Body Medium (Body Small) -->
			<StaticResource x:Key="BodyMediumFontFamily" ResourceKey="MaterialRegularFontFamily" />
			<x:String x:Key="BodyMediumFontWeight">Normal</x:String>
			<x:Double x:Key="BodyMediumFontSize">14</x:Double>
			<x:Int32 x:Key="BodyMediumCharacterSpacing">250</x:Int32>

			<!-- Body Small -->
			<StaticResource x:Key="BodySmallFontFamily" ResourceKey="MaterialRegularFontFamily" />
			<x:String x:Key="BodySmallFontWeight">Normal</x:String>
			<x:Double x:Key="BodySmallFontSize">12</x:Double>
			<x:Int32 x:Key="BodySmallCharacterSpacing">400</x:Int32>

		</ResourceDictionary>
	</ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
