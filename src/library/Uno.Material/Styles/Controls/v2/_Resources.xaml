	<StaticResource x:Key="SecondaryCheckBoxStyle"				ResourceKey="MaterialSecondaryCheckBoxStyle"		/>
	<StaticResource x:Key="SecondaryFabStyle"					ResourceKey="MaterialSecondaryFabStyle"				/>
