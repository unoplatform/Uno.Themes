	<StaticResource x:Key="IconToggleButtonStyle"				ResourceKey="MaterialIconToggleButtonStyle"			/>
	<StaticResource x:Key="LabelExtraSmall"						ResourceKey="MaterialLabelExtraSmall"				/>
