<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:controls="using:Uno.Material.Controls"
					xmlns:material="using:Uno.Material">

	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary>

			<!-- Card -->
			<Style TargetType="controls:Card"
				   BasedOn="{StaticResource MaterialOutlinedCardStyle}" />

			<!-- SnackBar -->
			<Style TargetType="controls:SnackBar"
				   BasedOn="{StaticResource MaterialSnackBarStyle}" />
		</ResourceDictionary>
	</ResourceDictionary.MergedDictionaries>
</ResourceDictionary>

